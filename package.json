{
  "name": "@examples-hub/code-playgrounds-js",
  "version": "1.1.1111",
  "private": true,
  "description": "examples for react projects and vanillajs projects",
  "keywords": [
    "template",
    "quickstart"
  ],
  "license": "MIT",
  "author": "jinyaoo <jinyaoo@qq.com>",
  "workspaces": [
    "packages/*",
    "package-a"
  ],
  "scripts": {
    "clean": "rimraf \"packages/**/dist\"",
    "dev": "",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format:check": "npm run formatter -- --check",
    "help": "echo 'exec npm start to view the demo'",
    "lint": "npm run formatter:check",
    "lint:js": "",
    "lint:style": "",
    "prepare": "",
    "publish": "",
    "start": "cd packages/sample-vanilla-app-ts && npm start",
    "syncpack-fix": "syncpack fix-mismatches",
    "syncpack-ls": "syncpack list-mismatches",
    "test": "jest --config=jest.monorepo.js",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run formatter"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
